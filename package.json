    "@types/react-native-vector-icons": "4.2.0",
    "assert": "^1.4.1",
    "buffer": "5.2.0",
    "cycle-native-alert": "1.0.0",
    "cycle-native-android-local-notification": "1.0.1",
    "cycle-native-keyboard": "1.1.0",
    "cycle-native-linking": "1.0.0",
    "cycle-native-navigation": "4.2.0",
    "cycle-native-share": "1.0.0",
